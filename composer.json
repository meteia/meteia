{
    "authors": [
        {
            "email": "jason@mcneil.dev",
            "name": "Jason R. McNeil"
        }
    ],
  "bin": [
    "bin/meteia"
  ],
    "autoload": {
        "files": [
            "Meteia/Dulce/Functions/dd.php",
            "Meteia/Http/Functions/send.php",
            "Meteia/Polyfills/Arrays.php",
            "Meteia/Polyfills/Strings.php"
        ],
        "psr-4": {
            "Meteia\\Application\\": "Meteia/Application/.",
            "Meteia\\Bluestone\\": "Meteia/Bluestone/.",
            "Meteia\\Classy\\": "Meteia/Classy/.",
            "Meteia\\CommandLine\\": "Meteia/CommandLine/.",
            "Meteia\\Configuration\\": "Meteia/Configuration/.",
            "Meteia\\Database\\": "Meteia/Database/.",
            "Meteia\\DependencyInjection\\": "Meteia/DependencyInjection/.",
            "Meteia\\Dulce\\": "Meteia/Dulce/.",
            "Meteia\\Html\\": "Meteia/Html/.",
            "Meteia\\Http\\": "Meteia/Http/.",
            "Meteia\\Logging\\": "Meteia/Logging/.",
            "Meteia\\Performance\\": "Meteia/Performance/.",
            "Meteia\\Polyfills\\": "Meteia/Polyfills/.",
            "Meteia\\ValueObjects\\": "Meteia/ValueObjects/."
        }
    },
    "name": "meteia/meteia",
    "require": {
        "aura/sql": "dev-psr-log-v3",
        "doctrine/inflector": "^2.0.4",
        "ext-pdo": "*",
        "laminas/laminas-diactoros": "^2.15.0",
        "nyholm/psr7": "^1.5.1",
        "nyholm/psr7-server": "^1.0.2",
        "php": "^8.1",
        "psr/container": "^2.0.2",
        "psr/http-factory": "^1.0.1",
        "psr/http-message": "^1.0.1",
        "psr/http-server-handler": "^1.0.1",
        "psr/http-server-middleware": "^1.0.1",
        "psr/log": "^3.0.0",
        "symfony/console": ">=v6.1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "stephank/composer-plugin-nixify": "*",
        "symplify/monorepo-builder": "^11.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jasonrm/Aura.Sql"
        }
    ],
    "config": {
        "allow-plugins": {
            "stephank/composer-plugin-nixify": true
        }
    },
    "replace": {
        "meteia/application": "self.version",
        "meteia/bluestone": "self.version",
        "meteia/classy": "self.version",
        "meteia/command-line": "self.version",
        "meteia/configuration": "self.version",
        "meteia/database": "self.version",
        "meteia/dependency-injection": "self.version",
        "meteia/dulce": "self.version",
        "meteia/html": "self.version",
        "meteia/http": "self.version",
        "meteia/logging": "self.version",
        "meteia/performance": "self.version",
        "meteia/polyfills": "self.version",
        "meteia/value-objects": "self.version"
    },
    "version": "0.0.1"
}
